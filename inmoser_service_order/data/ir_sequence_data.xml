<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Client Sequence -->
        <record id="seq_service_client" model="ir.sequence">
            <field name="name">Service Client Sequence</field>
            <field name="code">res.partner.x_inmoser_client_sequence</field>
            <field name="prefix">CLI</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next_actual">1</field>
            <field name="implementation">standard</field>
            <field name="use_date_range">False</field>
        </record>

        <!-- Equipment Sequence -->
        <record id="seq_service_equipment" model="ir.sequence">
            <field name="name">Service Equipment Sequence</field>
            <field name="code">inmoser.service.equipment</field>
            <field name="prefix">E</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next_actual">1</field>
            <field name="implementation">standard</field>
            <field name="use_date_range">False</field>
        </record>

        <!-- Service Order Sequence -->
        <record id="seq_service_order" model="ir.sequence">
            <field name="name">Service Order Sequence</field>
            <field name="code">inmoser.service.order</field>
            <field name="prefix">OS</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next_actual">1</field>
            <field name="implementation">standard</field>
            <field name="use_date_range">False</field>
        </record>
    </data>
</odoo>
